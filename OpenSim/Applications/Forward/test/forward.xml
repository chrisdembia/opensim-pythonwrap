<?xml version="1.0" encoding="UTF-8"?>
<InvestigationForward name="test">
	<defaults/>
	<model_library>rdBlock</model_library>
	<model_file> </model_file>
	<actuator_set_file> block_actuators.xml </actuator_set_file>
	<contact_force_set_file> block_contacts.xml </contact_force_set_file>
	<params_file> </params_file>
	<library_list> rdActuators suAnalyses </library_list>
	<results_directory> .</results_directory>
	<output_precision> 8 </output_precision>
	<initial_time>0.0</initial_time>
	<final_time>10.0</final_time>
	<maximum_number_of_integrator_steps> 20000 </maximum_number_of_integrator_steps>
	<maximum_integrator_step_size>1.0</maximum_integrator_step_size>
	<integrator_error_tolerance>0.001</integrator_error_tolerance>
	<integrator_fine_tolerance>0.00001</integrator_fine_tolerance>
	<AnalysisSet name="Analyses">
		<objects>
			<Kinematics name="Kinematics">
				<on> true </on>
				<start_time> -1.0e-30 </start_time>
				<end_time> 1.0e30 </end_time>
				<step_interval> 1 </step_interval>
				<in_degrees> true </in_degrees>
			</Kinematics>
			<Actuation name="Actuation">
				<on> true </on>
				<start_time> -1000.00000000 </start_time>
				<end_time> 1000.00000000 </end_time>
				<step_interval> 1 </step_interval>
				<in_degrees> true </in_degrees>
			</Actuation>
			<ActuatorGeneralizedForces name="ActuatorGeneralizedForces">
				<on> true </on>
				<start_time> 0.0</start_time>
				<end_time> 10.00000000 </end_time>
				<step_interval> 1 </step_interval>
				<in_degrees> true </in_degrees>
				<actuators_list>actuator</actuators_list>
			</ActuatorGeneralizedForces>
			<BodyKinematics name="BodyKinematics">
				<on> true </on>
				<start_time> -10.00000000 </start_time>
				<end_time> 100.00000000 </end_time>
				<step_interval> 1 </step_interval>
				<in_degrees> true </in_degrees>
				<angular_velocity_local> true </angular_velocity_local>
			</BodyKinematics>
		</objects>
	</AnalysisSet>
	<controls_file_name> block_controls.xml </controls_file_name>
	<initial_states_file_name> block_states.sto </initial_states_file_name>
</InvestigationForward>

