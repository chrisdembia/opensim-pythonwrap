<?xml version="1.0" encoding="UTF-8"?>
<Subject name="testSubject">
	<defaults/>
	<!--File name for the nominal model.xml to use is specified here-->
	<GenericModelParameters name="">
		<!--name of xml file for nominal model-->
		<filename> nominalModel.xml </filename>
		<MarkerSet/>
	</GenericModelParameters>
	<!--Parameters to control nominal model scaling go here.-->
	<ScalingParameters name="">
		<!--Valid options are 'measurements', 'manualScale'-->
		<scalingOrder>measurements</scalingOrder>
		<!--Measurements consist of 'MarkerPairSet' and 'BodyScaleSet'-->
		<MeasurementSet>
			<measurement name="pelvis depth">
				<apply>true</apply>             
				<MarkerPairSet>
					<markerPair> 
						<markers>R.ASIS V.Sacral</markers> 
					</markerPair>
					<markerPair> 
						<markers>L.ASIS V.Sacral</markers> 
					</markerPair>
				</MarkerPairSet>
				<BodyScaleSet>
         			<bodyScale name="pelvis"> 
						<axes>X</axes> 
					</bodyScale>
				</BodyScaleSet>
			</measurement>
		</MeasurementSet>
					
		<!--Scale factors to be used for manual(user controlled) scaling-->
		<ScaleSet file=""/>
		<markerFile> subject001_static.trc </markerFile>
		<timeRange> 0.0 0.25 </timeRange>
		<!--Whether to preserve relative mass between segments-->
		<preserveMassDistribution> false </preserveMassDistribution>
		<!--name of SIMM joint file to write when done scaling.-->
		<outputJointFile> subject001_meas.jnt </outputJointFile>
		<!--name of SIMM muscle file to write when done scaling.-->
		<outputMuscleFile> subject001_meas.msl </outputMuscleFile>
		<!--name of OpenSim xml file to write when done scaling.-->
		<outputModelFile> subject001.xml </outputModelFile>
		<!--name of scales file to write when done scaling.-->
		<outputScaleFile> scaleSet_subject001.xml </outputScaleFile>
	</ScalingParameters>
	<!--Parameters pertinent to solving IK are specified here-->
	<IKParameters name="">
		<modelFile> Unassigned </modelFile>
		<MarkerSet/>
		<CoordinateSet/>
		<IKTrialSet/>
	</IKParameters>
</Subject>

