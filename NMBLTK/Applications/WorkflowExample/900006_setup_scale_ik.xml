<?xml version="1.0" encoding="UTF-8" ?>
<Subject name="900006">
	<mass> 45.5 </mass>
	<!--File name for the nominal model.xml to use is specified here-->
	<GenericModelParameters name="">
		<!--name of xml file for nominal model-->
		<filename> gait.sim </filename>
		<MarkerSet file="gait_markers.xml" />
	</GenericModelParameters>
	<!--Parameters to control nominal model scaling go here.-->
	<ScalingParameters name="">
		<!--Valid options are 'measurements', 'manual'-->
		<scalingOrder> measurements </scalingOrder>
		<!--Measurements consist of 'MarkerPairSet' and 'BodyScaleSet'-->
		<MeasurementSet>
			<measurement name="measTorso">
				<apply>true</apply> <!-- set to false to not use this measurement -->
				<MarkerPairSet>
					<markerPair>
						<markers>midasis neck</markers>
					</markerPair>
				</MarkerPairSet>
				<BodyScaleSet>
					<bodyScale name="torso">
						<axes>X Y Z</axes>
					</bodyScale>
				</BodyScaleSet>
			</measurement>
			<measurement name="measPelvis">
				<apply>true</apply> <!-- set to false to not use this measurement -->
				<MarkerPairSet>
					<markerPair>
						<markers>midasis midhip</markers>
					</markerPair>
				</MarkerPairSet>
				<BodyScaleSet>
					<bodyScale name="pelvis">
						<axes>X Y Z</axes>
					</bodyScale>
				</BodyScaleSet>
			</measurement>
			<measurement name="measShank">
				<apply>true</apply> <!-- set to false to not use this measurement -->
				<MarkerPairSet>
					<markerPair>
						<markers>hip_r knee_r</markers>
						<markers>hip_l knee_l</markers>
					</markerPair>
				</MarkerPairSet>
				<BodyScaleSet>
					<bodyScale name="femur_r"> <axes>X Y Z</axes> </bodyScale>
					<bodyScale name="femur_l"> <axes>X Y Z</axes> </bodyScale>
					<bodyScale name="patella_r"> <axes>X Y Z</axes> </bodyScale>
					<bodyScale name="patella_l"> <axes>X Y Z</axes> </bodyScale>
				</BodyScaleSet>
			</measurement>
			<measurement name="measCalf">
				<apply>true</apply> <!-- set to false to not use this measurement -->
				<MarkerPairSet>
					<markerPair>
						<markers>knee_r ankle_r</markers>
						<markers>knee_l ankle_l</markers>
					</markerPair>
				</MarkerPairSet>
				<BodyScaleSet>
					<bodyScale name="tibia_r"> <axes>X Y Z</axes> </bodyScale>
					<bodyScale name="tibia_l"> <axes>X Y Z</axes> </bodyScale>
					<bodyScale name="talus_r"> <axes>X Y Z</axes> </bodyScale>
					<bodyScale name="talus_l"> <axes>X Y Z</axes> </bodyScale>
				</BodyScaleSet>
			</measurement>
			<measurement name="measFoot">
				<apply>true</apply> <!-- set to false to not use this measurement -->
				<MarkerPairSet>
					<markerPair>
						<markers>heel_r toe_r</markers>
						<markers>heel_l toe_l</markers>
					</markerPair>
				</MarkerPairSet>
				<BodyScaleSet>
					<bodyScale name="calcn_r"> <axes>X Y Z</axes> </bodyScale>
					<bodyScale name="calcn_l"> <axes>X Y Z</axes> </bodyScale>
					<bodyScale name="calcn_rp"> <axes>X Y Z</axes> </bodyScale>
					<bodyScale name="calcn_lp"> <axes>X Y Z</axes> </bodyScale>
					<bodyScale name="toes_r"> <axes>X Y Z</axes> </bodyScale>
					<bodyScale name="toes_l"> <axes>X Y Z</axes> </bodyScale>
				</BodyScaleSet>
			</measurement>
		</MeasurementSet>
		<!--Scale factors to be used for manual(user controlled) scaling-->
		<ScaleSet />
		<!--Name of marker file *.trc for static pose-->
		<markerFile> 900006_2.trc </markerFile>
		<!--Range of time in markers file to use for static pose solution-->
		<timeRange> 0.00833 2.00000</timeRange>
		<!--Whether to preserve relative mass between segments-->
		<preserveMassDistribution> true </preserveMassDistribution>
		<!--name of SIMM joint file to write when done scaling.-->
		<outputJointFile> 900006.jnt </outputJointFile>
		<!--name of SIMM muscle file to write when done scaling.-->
		<outputMuscleFile> 900006.msl </outputMuscleFile>
		<!--name of OpenSim xml file to write when done scaling.-->
		<outputModelFile> 900006_scaledOnly.sim </outputModelFile>
		<!--name of scales file to write when done scaling.-->
		<outputScaleFile> 900006_scaleSet_applied.xml </outputScaleFile>
	</ScalingParameters>
	<!--Marker set to override default markers is specified here-->
	<MarkerPlacementParameters name="">
		<markerFile> 900006_2.trc </markerFile>
		<timeRange> 0.00833  2.00000 </timeRange>
		<outputJointFile> 900006.jnt </outputJointFile>
		<!-- Name of the new model file after scaling and marker placement. This file will not be written if a name is not specfied.  -->
		<outputModelFile> 900006.sim </outputModelFile>
		<outputMarkerFile> 900006_markers.xml </outputMarkerFile>
		<outputMotionFile> 900006_markers.mot </outputMotionFile>
	</MarkerPlacementParameters>
	<!--Parameters pertinent to solving IK are specified here-->
	<IKParameters name="">
		<modelFile> Unassigned </modelFile>
		<MarkerSet />
		<CoordinateSet />
		<!--Trial paramaters, one block per trial-->
		<IKTrialSet />
	</IKParameters>
</Subject>
